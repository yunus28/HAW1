.root {
	-fx-background: black;
	-fx-font-size: 20pt;
}

.button {
	-fx-font-size: 40pt;
	-fx-background-color: white;
	-fx-background-radius: 0px;
	-fx-background-insets: 0px;
	-fx-padding: 0;
}

.button:hover {
	-fx-background-color: rgb(250, 250, 250);
}

.grid {
	-fx-background-color: black;
}

.label {
	-fx-font-color: white;
}

.text {
	-fx-font-smoothing-type: gray;
}